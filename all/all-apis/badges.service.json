        {
      "name": "_wix_badges_v4_Badge",
      "displayName": "Badge Object",
      "locations": [],
      "docs":       {
        "summary": "*\nA Badge is a a visible label to be displayed on a site member's profile.\nYou can use badges to create specific categories of members within your site.\nRead more about Badges\nin this [article](https://support.wix.com/en/article/about-member-badges).\n",
        "links": [],
        "examples": []
      },
      "members":       [
                {
          "name": "id",
          "type": "string",
          "doc": " Badge ID.\n",
          "readOnly": true
        },
                {
          "name": "revision",
          "type": "string",
          "doc": "*\nRevision number, which increments by 1 each time the Badge is updated.\nTo prevent conflicting changes,\nthe current revision must be passed when updating the Badge.\nIgnored when creating a Badge.\n",
          "readOnly": true
        },
                {
          "name": "title",
          "type": "string",
          "doc": " Text displayed on the badge in the Wix UI.\n"
        },
                {
          "name": "description",
          "type": "string",
          "doc": " Badge description.\n"
        },
                {
          "name": "backgroundColor",
          "type": "string",
          "doc": "\nBadge background color in hexadecimal RGB format.\nUppercase letters only.\nExample: `#FFFFFF`.\n"
        },
                {
          "name": "textColor",
          "type": "string",
          "doc": "\nBadge text color in hexadecimal RGB format.\nUppercase letters only.\nExample: `#C81B53`.\n"
        },
