    "docs":     {
      "summary": " Get published contents by key\n \n",
      "description": [],
      "request": "POST",
      "url": "https://www.wixapis.com/multilingual/v1/public-localized-content/by-key",
      "links": [],
      "examples": [      {
        "title": "",
        "body": ["curl -X POST 'https://www.wixapis.com/multilingual/v1/public-localized-content/by-key' \\\n   --data-binary \\\n   '{\n      \"content_keys\": [\n        {\n          \"schema_key\": {\n            \"app_id\": \"bd3b529d-547c-48d8-bc06-ef5ae19dc4b5\",\n            \"entity_type\": \"taco\"\n          },\n          \"entity_id\": \"001\"\n        },\n        {\n          \"schema_key\": {\n            \"app_id\": \"bd3b529d-547c-48d8-bc06-ef5ae19dc4b5\",\n            \"entity_type\": \"taco\"\n          },\n          \"entity_id\": \"002\"\n        }\n      ]\n    }\n   ' \\\n   -H 'Content-Type: application/json' \\\n   -H 'Authorization: <AUTH>'"],
        "response": ["{\"localized_published_contents\": [\n    {\n    \"content_id\": \"14cf373b-221e-476a-8268-6bffcb9dd945\",\n    \"content_key\":     {\n      \"schema_key\":       {\n        \"app_id\": \"bd3b529d-547c-48d8-bc06-ef5ae19dc4b5\",\n        \"entity_type\": \"taco\",\n        \"scope\": \"GLOBAL\"\n      },\n      \"entity_id\": \"001\"\n    },\n    \"published_fields_per_language\": [    {\n      \"language_tag\": \"en-US\",\n      \"language_published_fields\":       [\n                {\n          \"id\": \"title\",\n          \"string_value\": \"Chicken taco\",\n          \"sequence_path\": []\n        },\n                {\n          \"id\": \"description\",\n          \"string_value\": \"Chicken with mexican spices, rice, corn, sour cream\",\n          \"sequence_path\": []\n        }\n      ]\n    }]\n  },\n    {\n    \"content_id\": \"e617a52d-c76c-424e-ba91-b803044845f6\",\n    \"content_key\":     {\n      \"schema_key\":       {\n        \"app_id\": \"bd3b529d-547c-48d8-bc06-ef5ae19dc4b5\",\n        \"entity_type\": \"taco\",\n        \"scope\": \"GLOBAL\"\n      },\n      \"entity_id\": \"002\"\n    },\n    \"published_fields_per_language\":     [\n            {\n        \"language_tag\": \"en-US\",\n        \"language_published_fields\":         [\n                    {\n            \"id\": \"title\",\n            \"string_value\": \"New Beef taco\",\n            \"sequence_path\": []\n          },\n                    {\n            \"id\": \"description\",\n            \"string_value\": \"Carne asada, cheese & guac\",\n            \"sequence_path\": []\n          }\n        ]\n      },\n            {\n        \"language_tag\": \"he-IL\",\n        \"language_published_fields\":         [\n                    {\n            \"id\": \"title\",\n            \"string_value\": \"טאקו בשר חדש\",\n            \"sequence_path\": []\n          },\n                    {\n            \"id\": \"description\",\n            \"string_value\": \"אסאדו קון קרנה עם גבינה וגוואקמולי\",\n            \"sequence_path\": []\n          }\n        ]\n      }\n    ]\n  }\n]} "],
        "type": "curl",
        "extra": {}
      }],
      "extra": {}
    },
